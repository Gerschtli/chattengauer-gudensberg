		"node": "22.x"
	},
	"packageManager": "pnpm@10.8.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:unit run",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint --max-warnings=0 .",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"storyblok:pull-components": "pnpm storyblok pull-components --space 335007 && storyblok generate-typescript-typedefs --sourceFilePaths ./components.335007.json --destinationFilePath ./src/lib/component-types-storyblok.d.ts && pnpm run format",
