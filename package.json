		"eslint-plugin-svelte": "^3.5.1",
		"formsnap": "^2.0.1",
		"globals": "^16.0.0",
		"ical.js": "^2.1.0",
		"lucide-svelte": "^0.501.0",
		"nodemailer": "^6.10.1",
		"postcss": "^8.5.3",
