		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storyblok": "^3.36.0",
		"svelte": "^5.28.1",
		"svelte-check": "^4.1.6",
		"sveltekit-superforms": "^2.24.1",
