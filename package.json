		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.1.4",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.14.1",
		"@types/nodemailer": "^6.4.17",
		"@typescript-eslint/eslint-plugin": "^8.30.1",
